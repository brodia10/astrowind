{
    "version": 2,
    "name": "mycms",
    "buildCommand": "yarn build && yarn serve",
    "devCommand": "yarn dev",
    "outputDirectory": "build",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/admin"
      }
    ],
    "cleanUrls": true
  }