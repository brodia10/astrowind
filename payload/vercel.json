{
    "version": 2,
    "routes": [
        {
            "src": "/admin",
            "dest": "/admin/index.html",
            "methods": ["GET"],
            "headers": {
                "cache-control": "s-maxage=1, stale-while-revalidate"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/$1",
            "methods": ["GET"]
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ]
}
