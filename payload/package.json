{
  "name": "payload",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "openapi": {},
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "start": "node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "seed": "rm -rf media && cross-env PAYLOAD_SEED=true PAYLOAD_DROP_DATABASE=true PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node src/server.ts",
    "make:migration": "yarn payload migrate:create",
    "migrate:generate:types": "yarn generate:types && yarn make:migration"
  },
  "dependencies": {
    "@nouance/payload-better-fields-plugin": "^1.3.5",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-postgres": "^0.x",
    "@payloadcms/plugin-cloud": "^2.0.0",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-search": "^1.0.1",
    "@payloadcms/plugin-sentry": "^0.0.6",
    "@payloadcms/plugin-seo": "^1.0.15",
    "@payloadcms/plugin-stripe": "^0.0.19",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@types/nodemailer-sendgrid": "^1.0.3",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "https-browserify": "^1.0.0",
    "nodemailer": "^6.9.9",
    "nodemailer-postmark-transport": "^6.0.0",
    "os-browserify": "^0.3.0",
    "payload": "^2.11.1",
    "payload-plugin-comments": "^1.0.2",
    "payload-swagger": "^1.4.0",
    "postmark": "^4.0.2",
    "react": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "stripe": "^14.10.0",
    "svelte": "^4.2.12",
    "url": "^0.11.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/nodemailer": "^6",
    "@types/react": "^18",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
